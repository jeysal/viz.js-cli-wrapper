{
  "name": "viz.js-cli-wrapper",
  "version": "1.3.0",
  "description": "A small wrapper for viz.js that provides a command line interface similar to the original GraphViz",
  "keywords": [
    "GraphViz",
    "viz.js",
    "cli",
    "wrapper"
  ],
  "author": "Tim Seckinger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeysal/viz.js-cli-wrapper"
  },
  "files": [
    "src/main/"
  ],
  "main": "src/main/js/run.js",
  "bin": {
    "circo": "src/main/js/cli/circo.js",
    "dot": "src/main/js/cli/dot.js",
    "fdp": "src/main/js/cli/fdp.js",
    "neato": "src/main/js/cli/neato.js",
    "osage": "src/main/js/cli/osage.js",
    "twopi": "src/main/js/cli/twopi.js"
  },
  "scripts": {
    "test": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec src/test/js"
  },
  "dependencies": {
    "argparse": "^1.0.9",
    "mkdirp": "^0.5.1",
    "viz.js": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.1",
    "mock-stdin": "^0.3.1",
    "std-mocks": "^1.0.1",
    "tmp": "^0.0.33"
  }
}
